{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "OCR.response.v1",
  "type": "object",
  "required": [
    "raw_answer_text"
  ],
  "additionalProperties": false,
  "properties": {
    "raw_answer_text": {
      "type": "string",
      "minLength": 1,
      "description": "Transcription of student's handwritten/printed answer."
    },
    "confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    }
  }
}